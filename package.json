{
  "name": "eraser-map-web",
  "version": "1.0.0",
  "description": "mapzen web map",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "karma start test/karma.conf.js --log-level debug --single-run",
    "lint": "eslint ./src/js/*.js ./test/*.js; exit 0;",
    "build-style": "node-sass --output-style compressed -o dist/css src/scss; cp -R node_modules/leaflet-geocoder-mapzen/dist/images dist/css;",
    "dev": "browserify -t browserify-shim ./src/js/webmap.js --standalone WebMap > dist/bundle.min.js; cp src/index.html dist/; npm run-script build-style;",
    "build": "browserify -t browserify-shim ./src/js/webmap.js --standalone WebMap > dist/bundle.js; browserify -t browserify-shim -t uglifyify ./src/js/webmap.js --standalone  WebMap | uglifyjs -c > dist/bundle.min.js; cp src/index.html dist/; npm run-script build-style;"
  },
  "browserify-shim": {
    "leaflet": "global:L"
  },
  "dependencies": {
    "leaflet": "^0.7.7"
  },
  "devDependencies": {
    "benchmark": "^2.1.0",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "eslint": "^2.8.0",
    "eslint-config-semistandard": "^6.0.1",
    "eslint-config-standard": "^5.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "expect.js": "^0.3.1",
    "happen": "^0.3.1",
    "hashable": "^1.5.5",
    "istanbul": "^0.4.3",
    "karma": "^0.13.22",
    "karma-browserstack-launcher": "^0.1.11",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.5",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.2",
    "karma-nyan-reporter": "^0.2.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-requirejs": "^0.2.6",
    "karma-safari-launcher": "^0.1.1",
    "leaflet-geocoder-mapzen": "^1.6.2",
    "leaflet-hash": "^0.2.1",
    "mapzen-scarab": "git+https://github.com/mapzen/scarab.git",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "requirejs": "^2.2.0",
    "sinon": "^1.17.3",
    "uglifyify": "^3.0.1",
    "uglifyjs": "^2.4.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapzen/web-map.git"
  },
  "author": "MAPZEN",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapzen/web-map/issues"
  },
  "homepage": "https://github.com/mapzen/web-map#readme",
  "keywords": [
    "map",
    "mapzen",
    "eraser map",
    "map"
  ]
}
